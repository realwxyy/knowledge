{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      // 重要： 此处只配置src下的一级文件夹 多级文件夹配置转config-overrides中配置
      "@/*": [
        "*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@routes/*": [
        "routes/*"
      ],
      "@views/*": [
        "views/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    }
  }
}